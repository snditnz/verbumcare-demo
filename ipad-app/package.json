{
  "name": "ipad-app",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start --dev-client --lan",
    "start:clear": "expo start --dev-client --lan --clear",
    "dev": "expo start --dev-client --lan",
    "build:dev": "npx expo run:ios --device --configuration Debug",
    "build:dev:clean": "rm -rf ios/build && npx expo run:ios --device --configuration Debug",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "clean": "rm -rf node_modules .expo ios/Pods ios/build && watchman watch-del-all && npm cache clean --force",
    "clean:metro": "watchman watch-del-all && rm -rf $TMPDIR/react-* $TMPDIR/metro-* $TMPDIR/haste-map-*",
    "build:ios": "eas build --platform ios --profile development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:property": "jest property.test"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "11.4.1",
    "@react-native-community/slider": "4.5.5",
    "@react-navigation/native": "~6.1.18",
    "@react-navigation/native-stack": "~6.11.0",
    "axios": "~1.7.9",
    "buffer": "^6.0.3",
    "date-fns": "^4.1.0",
    "expo": "~52.0.48",
    "expo-av": "~15.0.0",
    "expo-camera": "~16.0.0",
    "expo-crypto": "~14.0.2",
    "expo-datetimepicker": "^1.0.3",
    "expo-dev-client": "~5.0.0",
    "expo-screen-orientation": "~8.0.4",
    "expo-status-bar": "~2.0.0",
    "react": "18.3.1",
    "react-native": "0.76.9",
    "react-native-ble-plx": "~3.2.1",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-live-audio-stream": "^1.1.1",
    "react-native-permissions": "^5.4.4",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "socket.io-client": "~4.8.1",
    "zustand": "~5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~18.3.12",
    "@types/react-native": "^0.73.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "fast-check": "^4.5.1",
    "jest": "~29.7.0",
    "react-test-renderer": "^18.3.1",
    "ts-jest": "^29.4.6",
    "typescript": "~5.6.2"
  },
  "private": true
}
